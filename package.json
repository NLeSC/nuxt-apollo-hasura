{
  "name": "emo-spectre",
  "version": "1.1.0",
  "description": "Nuxt Apollo Hasura Postgres recipes",
  "author": "Jesus Garcia Gonzalez",
  "organization": "NLeSC eScienceCenter",
  "private": true,
  "scripts": {
    "dev": "concurrently \"docker-compose up -d\" \"nuxt\"",
    "hasura-console": "cd hasura && hasura console --admin-secret adminpassword",
    "hasura-metadata-apply": "cd hasura && hasura metadata apply --admin-secret adminpassword",
    "stop": "docker-compose down",
    "start": "nuxt start",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "analyze": "yarn nuxt build --analyze",
    "migrate": "cd hasura && hasura migrate create hasurafiles",
    "release": "standard-version",
    "deploy:build": "docker-compose -f docker-compose.yml -f docker-compose-production.yml up -d --build",
    "deploy": "docker-compose -f docker-compose.yml -f docker-compose-production.yml up -d"
  },
  "dependencies": {
    "@mdi/js": "^5.5.55",
    "@nuxtjs/apollo": "^4.0.1-rc.3",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/pwa": "^3.0.2",
    "apexcharts": "^3.20.2",
    "d3": "^6.1.1",
    "d3fc": "^15.0.18",
    "idb": "^6.0.0",
    "js-sha256": "^0.9.0",
    "lodash.debounce": "^4.0.8",
    "nanoid": "^3.1.20",
    "nuxt": "^2.14.5",
    "vue-apexcharts": "^1.6.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "^7.11.2",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": ">=1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "hasura-cli": "^1.3.3",
    "prettier": "^2.1.2",
    "standard-version": "^9.0.0"
  }
}
